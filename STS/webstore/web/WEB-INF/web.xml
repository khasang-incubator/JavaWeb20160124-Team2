<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>spring-dispatcher</servlet-name>
        <servlet-class> org.springframework.web.servlet.DispatcherServlet
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>spring-dispatcher</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>dropTable</servlet-name>
        <servlet-class>io.khasang.webstore.model.DropTable</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dropTable</servlet-name>
        <url-pattern>/drop</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>menuHelper</servlet-name>
        <servlet-class>io.khasang.webstore.model.MenuHelper</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>menuHelper</servlet-name>
        <url-pattern>/menuHelper</url-pattern>
    </servlet-mapping>

</web-app>